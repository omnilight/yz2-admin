{
    "name": "omnilight/yz2-admin",
    "description": "Module for managing administration panel",
    "keywords": ["yii", "framework", "yz", "administation panel"],
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Pavel Agalecky",
            "email": "pavel.agalecky@gmail.com"
        }
    ],
    "type": "yii2-extension",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/omnilight/yz2-admin"
        },
        {
            "type": "vcs",
            "url": "https://github.com/omnilight/icons"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "omnilight/icons": "*",
        "omnilight/yz2": "*"
    },
    "autoload": {
        "psr-0": { "yz\\admin\\": "" }
    },
    "target-dir": "yz/admin",
    "extra": {
        "bootstrap": "yz\\admin\\Extension"
    }
}